    </BehaviorDescriptions>
    <Dialogs />
    <Resources>
        <File name="index" src="html/index.html" />
        <File name="README" src="README.md" />
        <File name="animate" src="html/css/animate.css" />
        <File name="chatui" src="html/css/chatui.css" />
        <File name="magic" src="html/css/magic.css" />
        <File name="arrow" src="html/img/arrow.png" />
        <File name="arrowBlue" src="html/img/arrowBlue.png" />
        <File name="arrowGray" src="html/img/arrowGray.png" />
        <File name="happy" src="html/img/happy.png" />
        <File name="icon_default" src="html/img/icon_default.png" />
        <File name="icon_pepper" src="html/img/icon_pepper.png" />
        <File name="icon_pepper_nega" src="html/img/icon_pepper_nega.jpg" />
        <File name="icon_pepper_posi" src="html/img/icon_pepper_posi.jpg" />
        <File name="icon_user" src="html/img/icon_user.png" />
        <File name="loading2" src="html/img/loading2.gif" />
        <File name="normal" src="html/img/normal.png" />
        <File name="unhappy" src="html/img/unhappy.png" />
